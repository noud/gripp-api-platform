    {
        return $this->purchaseinvoice;
    }

    public function setPurchaseinvoice(?Inkoopfactuur $purchaseinvoice): self
    {
        $this->purchaseinvoice = $purchaseinvoice;

        return $this;
    }

    public function getPurchaseorder(): ?Inkoopopdracht
    {
        return $this->purchaseorder;
    }

    public function setPurchaseorder(?Inkoopopdracht $purchaseorder): self
    {
        $this->purchaseorder = $purchaseorder;

        return $this;
    }

    public function getTask(): ?Taak
    {
        return $this->task;
    }

    public function setTask(?Taak $task): self
    {
        $this->task = $task;

        return $this;
    }


