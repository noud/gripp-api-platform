 * @ORM\Entity
 */
class Timelineentry extends \App\Entity\AbstractEntity\AbstractNameAndExtendedPropertiesEntity
{
    /**
     * @var Medewerker|null
     *
     * @ORM\ManyToOne(targetEntity="App\Entity\Medewerker")
     * @ORM\JoinColumn(name="employee", referencedColumnName="id")
     */
    private $employee;

    /**
     * @var \DateTime|null
     *
     * @ORM\Column(name="date", type="datetime", nullable=true)
     */
    private $date;

    /**
     * @var string|null
     *
     * @ORM\Column(name="message", type="string", length=255, nullable=true)
     */
    private $message;

    /**
     * @var string|null
     *
     * @ORM\Column(name="timelinetype", type="string", length=0, nullable=true)
     */
    private $timelinetype;

    /**
     * @var bool|null
     *
     * @ORM\Column(name="starred", type="boolean", nullable=true)
     */
    private $starred;

    /**
     * @var int|null
     *
     * @ORM\Column(name="files", type="bigint", nullable=true)
     */
