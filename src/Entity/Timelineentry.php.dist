     */
    private $company;

    /**
     * @var \Contactpersoon
     *
     * @ORM\ManyToOne(targetEntity="Contactpersoon")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="contact", referencedColumnName="id")
     * })
     */
    private $contact;

    /**
     * @var \Contract
     *
     * @ORM\ManyToOne(targetEntity="Contract")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="contract", referencedColumnName="id")
     * })
     */
    private $contract;

    /**
     * @var \Factuur
     *
     * @ORM\ManyToOne(targetEntity="Factuur")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="invoice", referencedColumnName="id")
     * })
     */
    private $invoice;

    /**
     * @var \Offerte
     *
     * @ORM\ManyToOne(targetEntity="Offerte")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="offer", referencedColumnName="id")
     * })
     */
    private $offer;

    /**
     * @var \Opdracht
     *
     * @ORM\ManyToOne(targetEntity="Opdracht")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="project", referencedColumnName="id")
     * })
     */
    private $project;

    /**
